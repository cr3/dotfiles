# .mailcap
application/octet-stream; filter-octet %s; copiousoutput
application/x-gzip; filter-octet %s; copiousoutput
application/zip; filter-octet %s; copiousoutput
text/x-vcard; filter-vcard; copiousoutput
text/html; w3m -T text/html -I %{charset} %s; needsterminal; description=HTML Text; nametemplate=%s.html
text/html; w3m -dump -T text/html -I %{charset} %s; copiousoutput; description=HTML Text; nametemplate=%s.html
application/msword; wvHtml %s - 2>/dev/null | w3m -T text/html -cols 80 -dump | tr -d \\240; copiousoutput; nametemplate=%s.doc
application/vnd.ms-word; wvHtml %s - 2>/dev/null | w3m -T text/html -cols 80 -dump | tr -d \\240; copiousoutput; nametemplate=%s.doc
application/x-msword; wvHtml %s - 2>/dev/null | w3m -T text/html -cols 80 -dump | tr -d \\240; copiousoutput; nametemplate=%s.doc
application/msexcel; xlhtml %s 2>/dev/null | w3m -T text/html -cols 80 -dump | tr -d \\240; copiousoutput; nametemplate=%s.xls
application/vnd.ms-excel; xlhtml %s 2>/dev/null | w3m -T text/html -cols 80 -dump | tr -d \\240; copiousoutput; nametemplate=%s.xls
application/x-msexcel; xlhtml %s 2>/dev/null | w3m -T text/html -cols 80 -dump | tr -d \\240; copiousoutput; nametemplate=%s.xls
